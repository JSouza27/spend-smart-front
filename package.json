{
  "name": "spend-smart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "lint:next": "next lint",
    "prepare": "husky install",
    "test": "jest --maxWorkers=25%",
